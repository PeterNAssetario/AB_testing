{
	"name": "Ab Testing",
	"dockerComposeFile": [
		"docker-compose.yaml"
	],
	"service": "dev",
	"workspaceFolder": "/home/node/app",
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},
